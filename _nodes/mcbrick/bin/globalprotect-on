#!/usr/bin/env osascript

on run
  -- Click the menu bar item.
  delay 0.5
  set uiScript to "click menu bar item 1 of menu bar 2 of application process \"GlobalProtect\""
  my doIt( uiScript )

  -- Click the “Connect” button.
  delay 0.5
  set uiScript to "click UI Element \"Connect\" of window 1 of application process \"GlobalProtect\""
  my doIt( uiScript )

  -- Click the menu bar item.
  delay 0.5
  set uiScript to "click menu bar item 1 of menu bar 2 of application process \"GlobalProtect\""
  my doIt( uiScript )
end run

on doIt(uiScript)
  try
    run script "tell application \"System Events\"
" & uiScript & "
end tell"
  on error errorMessage
    do shell script "echo " & uiScript + errorMessage
  end try
end doIt
